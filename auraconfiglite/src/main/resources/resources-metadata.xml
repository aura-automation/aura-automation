				</containment>
			</ConfigObject>
		</ConfigObject>

		<ConfigObject type="__ActivationSpec_JMS_ActivationSpec"
			shouldCreate="false">
			<containment>
				<attribute value="null" />
			</containment>
			<ConfigObject type="ActivationSpec" shouldCreate="false">
				<containment>
					<attribute value="null" />
				</containment>
			</ConfigObject>
		</ConfigObject>

	</ConfigObject>



</resources-metadata>
