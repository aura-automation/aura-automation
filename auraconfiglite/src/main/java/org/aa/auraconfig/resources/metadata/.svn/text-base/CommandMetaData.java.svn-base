package com.apartech.auraconfig.resources.metadata;

import java.util.HashMap;

public class CommandMetaData {

	private String createCommand;
	private String modifyCommand;
	private HashMap<String, CommandAttribute> attributeMappings;
	private StepCommandMetaData stepCommandMetaData; 
	/**
	 * @return the stepCommandMetaData
	 */
	public StepCommandMetaData getStepCommandMetaData() {
		return stepCommandMetaData;
	}
	/**
	 * @param stepCommandMetaData the stepCommandMetaData to set
	 */
	public void setStepCommandMetaData(StepCommandMetaData stepCommandMetaData) {
		this.stepCommandMetaData = stepCommandMetaData;
	}
	/**
	 * @return the createCommand
	 */
	public String getCreateCommand() {
		return createCommand;
	}
	/**
	 * @param createCommand the createCommand to set
	 */
	public void setCreateCommand(String createCommand) {
		this.createCommand = createCommand;
	}
	
	/**
	 * @return the modifyCommand
	 */
	public String getModifyCommand() {
		return modifyCommand;
	}
	/**
	 * @param modifyCommand the modifyCommand to set
	 */
	public void setModifyCommand(String modifyCommand) {
		this.modifyCommand = modifyCommand;
	}
	/**
	 * @return the attributeMappings
	 */
	public HashMap<String, CommandAttribute> getAttributeMappings() {
		return attributeMappings;
	}
	/**
	 * @param attributeMappings the attributeMappings to set
	 */
	public void setAttributeMappings(HashMap<String, CommandAttribute> attributeMappings) {
		this.attributeMappings = attributeMappings;
	}
	

}
