				<J2CActivationSpec
					activationSpec="${{__ActivationSpec_JMS_ActivationSpec}}">

					        <J2EEResourceProperty />

				</J2CActivationSpec>

			</J2CResourceAdapter>
		</ServerCluster>

	</Cell>


</resources>